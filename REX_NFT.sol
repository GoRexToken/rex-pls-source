    function generateSVG(uint256 tokenId, uint round, uint256 plsStore, address collection) internal pure returns (string memory svg) {

        svg = string(
            abi.encodePacked(
                '<svg width="290" height="500" viewBox="0 0 290 500" xmlns="http://www.w3.org/2000/svg" ',
                "xmlns:xlink='http://www.w3.org/1999/xlink'>",
                generateSVGDefs(tokenId, collection),
                generateSVGTokenID(tokenId, round, plsStore),
                getRexSizeSvg(plsStore),
                generateSVGLogo(),
                '</svg>'
            )
        );
    }

    function isRare(uint256 tokenId, address collection) internal pure returns (bool) {
        bytes32 h = keccak256(abi.encodePacked(tokenId, collection));
        return uint256(h) < type(uint256).max / (1 + BitMath.mostSignificantBit(tokenId) * 2);
    }

    function displayFullPls(uint256 val) public pure returns (string memory) {
        if (val == 0) { return string("0"); }
        if (val < 1000) { return val.toString(); }

        bytes memory byteArrayA = bytes( val.toString() );
        uint256 noOfCommata = (byteArrayA.length / 3);
        noOfCommata = byteArrayA.length % 3 == 0 ? noOfCommata - 1 : noOfCommata;
        uint256 newLength = byteArrayA.length + noOfCommata;
        bytes memory result = new bytes(newLength);

        uint j = 0;

        for (uint i = 0; i < result.length; i++) {
            if ((result.length - i) % 4 == 0) {
                result[i] = hex"27";
            }
            else {
                result[i] = byteArrayA[j]; j++;
            }
        }

        return string(result);
    }

