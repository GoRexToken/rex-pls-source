contract PlsRexNft is ERC721Royalty, ERC721Enumerable, AccessControl {

    using Counters for Counters.Counter;
    using Strings for uint256;

    Counters.Counter private _tokenIdCounter;

    address private deployer;
    address public TRAIT_CONTRACT;
    address public NFTSTAKING_CONTRACT;

    uint256 public constant ROUND_SIZE = 1000;
    uint256 public constant BASE_PRICE = 1E6 ether; // 1M PLS

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    // Mapping from token id to stored PLS
    mapping(uint256 => uint256) public plsStored;

    constructor(address _REX) ERC721("Feedable Rexagotchi", "REXAGOTCHI") {
        deployer = msg.sender;
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, _REX);
        _tokenIdCounter.increment(); // start count at 1
    }

    function init(address _NFTS, address _TRAIT) external {
        require(msg.sender == deployer, "No auth");
        _setDefaultRoyalty(_NFTS, 2500); // 25% suggested royalty
        NFTSTAKING_CONTRACT = _NFTS;
        TRAIT_CONTRACT = _TRAIT;
        deployer = address(0);
    }

        /// price of a certain NFT on the NFT marketplace (w/o decimals)
    function marketPriceShort(uint256 tokenId) public view returns (uint256) {
        return plsStored[tokenId] / 1E18;
    }

    /// price of the next NFT to be minted
    function mintPrice() public view returns (uint256) {
        return BASE_PRICE + ( BASE_PRICE / 20 * (totalSupply() / ROUND_SIZE) );
    }

    function mintPriceMany(uint256 count) external view returns (uint256) {
        if (count == 1) { return mintPrice(); }
        uint256 availableInRound = (ROUND_SIZE * ( (totalSupply() / ROUND_SIZE) + 1) ) - totalSupply();
        if (count <= availableInRound) {
            return (BASE_PRICE + ( BASE_PRICE / 20 * (totalSupply() / ROUND_SIZE)) ) * count;
        } else {
            uint256 partOne = (BASE_PRICE + ( BASE_PRICE / 20 * (totalSupply() / ROUND_SIZE) )) * availableInRound;
            return partOne + (BASE_PRICE + ( BASE_PRICE / 20 * ((totalSupply() + count) / ROUND_SIZE) )) * (count - availableInRound);
        }
    }

    function round() external view returns (uint256) {
        return (totalSupply() / ROUND_SIZE) + 1;
    }

    function roundOf(uint256 tokenId) public pure returns (uint256) {
        require(tokenId > 0, "wrong Id");
        return ((tokenId - 1) / ROUND_SIZE) + 1;
    }

    function safeMint(address to) public onlyRole(MINTER_ROLE) {
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(to, tokenId);
        plsStored[tokenId] = BASE_PRICE;
    }

    function addPlsToNft(uint256 tokenId, uint256 amount) external {
        require(msg.sender == NFTSTAKING_CONTRACT, "No auth");
        plsStored[tokenId] += amount;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721)
        returns (string memory)
    {
        _requireMinted(tokenId);
        return _constructTokenURI(tokenId, address(this));
    }

    function tokensOfOwner(address owner) public view returns (uint[] memory tokenIds) {
        uint balance = balanceOf(owner);
        require(balance > 0, "No balance");

        tokenIds = new uint[](balance);
        for (uint i=0; i < balance; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(owner, i);
        }
    }

    function _constructTokenURI(uint256 tokenId, address collection) internal view returns (string memory svg) {

        string memory image = NFTSVG.generateSVG(tokenId, roundOf(tokenId), marketPriceShort(tokenId), collection);

        image = Base64.encode(bytes(image));

        svg =
            string(
                abi.encodePacked(
                    'data:application/json;base64,',
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                'REXAGOTCHI #',
                                tokenId.toString(),
                                '", "description":"',
                                'This NFT is a living creature. Stake for yield. Feed to grow. ',
                                "The owner may resell it on a DEX. If sold, a 25% royalty will be paid to staker's treasury. ",
                                "Stake, feed, trade on pls-rex.io"
                                '", "image": "',
                                'data:image/svg+xml;base64,',
                                image,
                                '","attributes":[{"trait_type":"id","value":"',
                                tokenId.toString(),
                                '"},{"trait_type":"pls","value":"',
                                marketPriceShort(tokenId).toString(),
                                '"},{"trait_type":"rarity","value":"',
                                NFTSVG.isRare(tokenId, collection) ? 'rare' : 'common',
                                '"}]}'
                            )
                        )
                    )
                )
            );
    }


    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(address from, address to, uint256 tokenId, uint256 batchSize)
        internal
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Royalty, ERC721Enumerable, AccessControl)
        returns (bool)
    {
        return  interfaceId == type(ERC721Enumerable).interfaceId || interfaceId == type(AccessControl).interfaceId || interfaceId == type(ERC721Royalty).interfaceId || super.supportsInterface(interfaceId);
    }

    function _burn(uint256 tokenId) internal virtual override (ERC721, ERC721Royalty) {
        super._burn(tokenId);
    }
}
